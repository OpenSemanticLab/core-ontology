#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/created
dcterms:created rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/pmd/co/qualities/in_response_to
:in_response_to rdf:type owl:ObjectProperty ;
                owl:inverseOf :responds_with .


###  https://w3id.org/pmd/co/qualities/responds_with
:responds_with rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain obo:BFO_0000017 ;
               rdfs:range :Response ;
               rdfs:comment "The realizable entity must be \"stimulated\" by some Stimulus in order to respond with the Response. The bearer of the realizable entity must participate in Stimulation as well as in Response." ;
               rdfs:label "responds with"@en .


###  https://w3id.org/pmd/co/qualities/stimulated_by
:stimulated_by rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf obo:BFO_0000054 ;
               rdfs:range :Stimulation ;
               rdfs:label "stimulated by"@en .


###  https://w3id.org/pmd/co/qualities/stimulates
:stimulates rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf obo:BFO_0000055 ;
            rdfs:domain :Stimulation ;
            rdfs:label "stimulates"@en .


#################################################################
#    Classes
#################################################################

